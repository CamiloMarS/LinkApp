<div class="container p-4">
     <div class="row">
               <!-- Recorrer la lista de links de la consulta -->
               {{#each links_pages}}
                  <div class="col-md-4">
                       <div class="card text-center">
                            <div class="card-body">
                                 <a href="{{this.url}}" target="_blank">
                                      <h3 class="card-title text-uppercase">{{this.title}}</h3>
                                 </a>
                                 <p class="m-2">{{descripcion}}</p>
                                 <p class="">{{timeago this.created_at}}</p>
                                 <a href="#" class="btn btn-danger">Delete</a>
                                 <a href="#" class="btn btn-primary">Edit</a>
                            </div>
                       </div>
                  </div>
               {{/each}}
     </div>
</div>